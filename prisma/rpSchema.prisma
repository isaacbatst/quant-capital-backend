generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_RP")
}

model api_integracao {
  id         Int       @id @default(autoincrement())
  unit_id    Int
  gatway     Int
  sandbox    String    @db.VarChar(500)
  producao   String    @db.VarChar(500)
  chave      String    @db.VarChar(500)
  credencial String    @db.VarChar(500)
  created_at DateTime  @default(now()) @db.Timestamp(0)
  updated_at DateTime  @default(now()) @db.Timestamp(0)
  deleted_at DateTime? @db.Timestamp(0)

  @@unique([unit_id, gatway], map: "gatway_unico_pk")
}

model banco {
  id          Int         @id @default(autoincrement())
  num_banco   String      @db.VarChar(11)
  nome_banco  String      @db.VarChar(55)
  unit_id     Int
  created_at  DateTime    @default(now()) @db.Timestamp(0)
  updated_at  DateTime    @default(now()) @db.Timestamp(0)
  deleted_at  DateTime?   @db.Timestamp(0)
  system_unit system_unit @relation(fields: [unit_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Registro possui movimentação")

  @@index([unit_id], map: "unit_id")
}

